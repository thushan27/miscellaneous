   "source": [
    "\"\"\"\n",
    "CyclicRotation -  Rotate an array A to the right by a K number of steps. \n",
    "\"\"\""
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "ad24f251",
   "metadata": {},
   "outputs": [],
   "source": [
    "def cyclicRotation(a, k):\n",
    "   \n",
    "    n = len(a)\n",
    "    a[:] = a[n-k:]+a[:n-k]\n",
    "        \n",
    "    return a"
   ]
  },
